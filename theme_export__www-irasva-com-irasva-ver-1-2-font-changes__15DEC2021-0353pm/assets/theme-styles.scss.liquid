.icon-nav {
  border: none;
  outline: none;
  padding: 0;
  display: block;
  width: 24px;
  height: 24px;
  position: relative;


  .icon-line {
    display: block;
    width: 24px;
    height: 2px;
    margin: 3px 0;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    background-color: #fff;

    &:after {
      background-color: #fff;
      display: block;
      width: 13px;
      height: 2px;
      margin: 3px 0;
      position: absolute;
      left: 0;
      right: 0;
      content: "";
      @include transition(transform .3s);
      @include transition-delay(.2s);
    }

    &:before {
      top: 5px;
      background-color: #fff;
      display: block;
      width: 18px;
      height: 2px;
      margin: 3px 0;
      position: absolute;
      left: 0;
      right: 0;
      content: "";
      @include transition(transform .3s);
      @include transition-delay(.2s);
    }

    &:after {
      top: 12px;
    }
  }
}

