.product-name {
  color: $color_title_pr;
  display: block;
  margin-bottom: 4px;
  text-transform: $product_text_transform;
  font-size: 14px;
  line-height: 26px;
  font-weight: normal;
  font-family:$header_font_regular;

  &:hover {
    color: $color_title_pr_hover;
  }
}

.price-box {
  font-size: $font_size + 1;
  font-weight: 600;
  line-height: $font_size + 10;
  letter-spacing: normal;
  margin-bottom: 12px;

  .price-regular {
    color: $color_price;
  }

  .old-price {
    color: $color_compare_price;
    text-decoration: line-through;
    margin-right: 5px;
  }

  .special-price {
    color: $color_price_sale;
  }

  em {
    display:none;
  }
}

.spr-icon-star-empty:before,
.spr-icon.spr-icon-star:before,
.spr-icon:before {
  color: $page_title_color;
}

.spr-badge-starrating {
  margin-right: 0 !important;
}

.spr-starrating.spr-summary-starrating {
  margin-right: 10px;
}

.spr-icon {
  padding: 0 2px;
  font-size: 105% !important;
  margin: 0;

  &:first-of-type {
    padding-left: 0;
  }

  &.spr-icon-star,
  &.spr-icon-star-hover {
    content: '\e800' !important;
  }

  &.spr-icon-star-empty,
  &.spr-icon-star-half-alt {
    opacity: 1 !important;
  }
}

[class^="spr-icon-"]:before,
[class*=" spr-icon-"]:before {
  font-size: 100% !important;
}

.grid-item {
  &.grid-item-border {
    .product-image {
      border: 1px solid {{ settings.product_image_border_color }};
      padding: 10px;
    }
  }
}

.product-img-box {
  &.grid-item-border {
    border: 1px solid {{ settings.product_image_border_color }};
    padding: 10px;
  }
}

.products-grid {
  .wishlist-text {
    display: none;
  }

  .grid-item {
    padding: 18px;
    .not_img {
      height: 270px;
      line-height: 270px;
      font-size: 18px;
    }

    .no-product {
      .product-top {
        margin-bottom: 10px;
      }
    }

    .product-details {
      display: none!important;
    }

    &.banner-img {
      img {
        width: 100%;
        object-fit: cover;
      }
    }

    &:not(.grid-item-mansory) {
      {% if settings.product_image_resize %}
      .product-image {
        overflow:hidden;
        img {
          position: absolute!important;
          top: 50%;
          left: 50%;
          @include translate(-50%, -50%);
          object-fit: contain;
          {% if settings.product_image_crop %}
          height:100%;
          width:auto;
          max-width:none;
          {% else %}
          height: 100%;
          max-height: 100%;
          width: 100%;
          max-width: 100%;
          {% endif %}

        }
      }

      .product-grid-image {
        position: relative;

        &:after {
          content: "";
          display: block;
          width: 100%;
          height: 0;
          padding-top: {{settings.product_image_height}}%;
        }
      }
      {% endif %}
    }
  }

  .product-image {
    .product-grid-image {
      min-height: 216px;
    }

    img {
      min-height: 216px;
    }
  }

  &.grid-2,
  &.grid-3,
  &.grid-4,
  &.grid-5 {
    display: block;
    -webkit-column-gap: 0;
    -moz-column-gap: 0;
    column-gap: 0;
    padding-bottom: 40px;

    .grid-item {
      break-inside: avoid;
      break-inside: avoid-column;
      page-break-inside: avoid;
      -webkit-column-break-inside: avoid;
      -webkit-box-flex: 0;
      -ms-flex: 0 0 100%;
      flex: 0 0 100%;
      max-width: 100%;
    }
  }

  &.grid-2 {
    -moz-column-count: 2;
    -webkit-column-count: 2;
    column-count: 2;
  }
}

{% if settings.product_image_resize %}
.sidebar-widget-product {
  .products-grid .grid-item {
    .product-grid-image {
      &:after {
        padding-top: 160px;
      }
    }
  }
}
{% endif %}

.product-item {
  .product-top {
    position: relative;
    text-align: center;
    .wishlist-absolute{
      position: absolute;
      top: 0;
      right: 8px;
    }
  }

  .product-grid-image {
    position: relative;
    display: block;
  }

  .product-image {
    margin-bottom: 0;

    img {
      object-fit: contain;
      max-height: 100%;
      {% comment %} box-shadow: 0 3px 8px -3px rgba(0, 0, 0, 0.5); {% endcomment %}
    }
  }

  .wishlist {
    margin-bottom: 15px;
    display: inline-block;
    color: $color_wishlist;

    .fa {
      width: 26px;
      height: 26px;
      /*       background-color: $bg_wishlist; */
      color: $color_wishlist;
      @include border-radius(50%);
      margin: 0;
      font-size: 20px;
      line-height: 13px;
      text-align: center;
      padding: 7px 6px 6px;
    }

    &.whislist-added {
      .fa {
        color: $color_price_sale;
      }
    }
  }

  .spr-badge {
    margin-bottom: 9px;

    .spr-badge-starrating {
      &+.spr-badge-caption {
        display: none;
      }
    }
  }

  .action {
    margin-bottom: 12px;
  }

  .btn {
    @include button_2();
    width: 100%;

    &:hover {
      @include button_1();
    }
  }

  .product-bottom {
    text-align: $product_name_text_align;
    padding: 10px 6px;
    background: #FAFAFA;
    @media ( min-width: 1200px ) and ( max-width: 1400px ){
      // height: 100px;
    }
  }

  .product-des {
    text-align: right;
    position: absolute;
    left: 0;
    right: 0;
    z-index: 2;
    @include opacity(1);
    @include transition(opacity .5s ease);
    visibility: visible;

    &.abs-center {
      top: 7%;
      right: 3%;
      @include translateY(-50%);
    }

    &.abs-bottom {
      bottom: 0;
    }
  }
}

.infinite-scrolling-homepage,
.infinite-scrolling {
  text-align: center;

  .btn {
    width: 100%;
    max-width: 300px;
    background: $background_2;
    color: $color_2;
    border-color: #727272;

    &:hover {
      background: transparent;
      color: #de7d77;
      border-color: transparent;
      text-decoration: underline;
    }

    &.disabled {
      color: $sold_out_text !important;
      background: $bg_sold_out !important;
      border-color: $bg_sold_out !important;
      cursor: default;
      pointer-events: none;
    }
  }
}

