      @include flexbox();
      @include justify-content(space-between);
      @include align-items(center);
      overflow: hidden;
      text-overflow: ellipsis;
      -webkit-line-clamp: 1;
      -webkit-box-orient: vertical;
      cursor: pointer;
      font-weight:300;
      font-size: 14px;
      color: #C4C9D1;
      letter-spacing: 0;
    }

    .icon-dropdown {
      color: $color_icon_drop;
      font-size: 16px;
      padding-left: 8px;
    }

    .dropdown-menu {
      padding: 5px 0;
      margin: 2px 0;
      right: 0;
      @include border-radius(0);
      font-size: $font_size;
      color: $body_color;
      min-width: 100%;

      li {
        * {
          display: block;
          padding: 4px 14px 3px 15px;
          cursor: pointer;
          white-space: nowrap;
          overflow: hidden;
          text-overflow: ellipsis;
        }

        &.active,
        &:hover {
          * {
            background-color: #f4f4f4;
            color: #66787f;
          }
        }
      }
    }
  }

  .filters-toolbar__limited-view {
    padding-right: 15px;

    .limited-view {
      min-width: 89px;
      position: relative;
    }
  }

  .filters-toolbar__sortby {
    padding-left: 15px;

    .filter-sortby {
      min-width: 149px;
      position: relative;
    }
  }

  .view-mode {
    padding: 5px 0;
    @include flexbox();

    .icon-mode {
      min-width: 19px;
      height: 26px;
      display: block;
      border: 1px solid #dcdcdc;
      margin-left: 5px;
      position: relative;
      cursor: pointer;

      &:first-of-type {
        margin-left: 0;
      }

      &:before {
        background: #dcdcdc;
        content: "";
        top: 3px;
        position: absolute;
        left: 3px;
      }

      &.icon-mode-list,
      &.grid-3 {
        width: 26px;
      }

      &.icon-mode-list {
        &:before {
          width: 18px;
          height: 4px;
          box-shadow: 0px 7px 0 #dcdcdc, 0px 14px 0 #dcdcdc;
        }
      }

      &.icon-mode-grid {
        &:before {
          width: 4px;
          height: 18px;
        }
      }

      &.grid-2 {
        width: 19px;

        &:before {
          box-shadow: 7px 0 0 #dcdcdc;
        }
      }

      &.grid-3 {
        &:before {
          box-shadow: 7px 0 0 #dcdcdc, 14px 0 0 #dcdcdc;
        }
      }

      &.grid-4 {
        width: 33px;

        &:before {
          box-shadow: 7px 0 0 #dcdcdc, 14px 0 0 #dcdcdc, 21px 0 0 #dcdcdc;
        }
      }

      &.grid-5 {
        width: 40px;

        &:before {
          box-shadow: 7px 0 0 #dcdcdc, 14px 0 0 #dcdcdc, 21px 0 0 #dcdcdc, 28px 0 0 #dcdcdc;
        }
      }

      &.active {
        border-color: $color_slick_arrow;

        &:before {
          background-color: $color_slick_arrow;
        }

        &.icon-mode-list {
          &:before {
            box-shadow: 0px 7px 0 $color_slick_arrow, 0px 14px 0 $color_slick_arrow;
          }
        }

        &.grid-2 {
          &:before {
            box-shadow: 7px 0 0 $color_slick_arrow;
          }
        }

        &.grid-3 {
          &:before {
            box-shadow: 7px 0 0 $color_slick_arrow, 14px 0 0 $color_slick_arrow;
          }
        }

        &.grid-4 {
          &:before {
            box-shadow: 7px 0 0 $color_slick_arrow, 14px 0 0 $color_slick_arrow, 21px 0 0 $color_slick_arrow;
          }
        }

        &.grid-5 {
          &:before {
            box-shadow: 7px 0 0 $color_slick_arrow, 14px 0 0 $color_slick_arrow, 21px 0 0 $color_slick_arrow, 28px 0 0 $color_slick_arrow;
          }
        }
      }
    }
  }

  .sidebar-label {
    .icon-filter {
      svg {
        height: 27px;
        padding-left: 2px;
        width: 24px;
      }
    }
  }
