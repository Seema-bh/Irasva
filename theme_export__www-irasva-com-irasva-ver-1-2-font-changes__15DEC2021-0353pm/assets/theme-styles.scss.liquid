    margin-bottom: 0;
    text-transform: capitalize;
    position: relative;
    padding-bottom: 18px;

    .line {
      position: absolute;
      left: 50%;
      bottom: 0;
      @include translateX(-50%);
      height: 2px;
      width: 40px;
    }
  }

  .banner-des {
    text-transform: uppercase;
    max-width: 425px;
