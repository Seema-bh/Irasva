  .panel-group {
    margin-bottom: 50px;
    word-break: break-word;

    .panel-default {
      margin-bottom: 15px;
    }

    .panel-title a,
    .panel-body {
      padding: 9px 25px 7px 12px;
      @include flexbox();
      @include align-items(flex-start);

      &:before {
        display: inline-block;
        font-size: $font_size - 2;
        font-weight: 500;
        line-height: 1em;
        padding: 4px 8px 3px;
        min-width: 63px;
        @include border-radius(1px);
        margin-right: 20px;
        text-align: center;
      }
