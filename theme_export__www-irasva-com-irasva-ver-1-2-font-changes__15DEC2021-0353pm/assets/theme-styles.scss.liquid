    }
  }
}

.no-js {
  .product-img-box {
    .video-open-popup {
      @include opacity(0);
    }
  }
}

.product-img-box {
  position: relative;

  .slider-for {
    margin-bottom: 30px;
    position: relative;

    .zoomImg{
      cursor: crosshair;
    }

    .thumb {
      text-align: center;
    }

    [data-zoom] {
      display: block;
    }

    &:not(.slick-slider) {
      .thumb {
        {% if settings.use_color_swatch and settings.color_swatch_style == 'variant_grouped' %}
        @include opacity(0);
        {% endif %}

        &+.thumb {
          display: none;
        }
      }
    }
  }

  .slider-nav {
    padding: 0 35px;
    margin-bottom: 22px;
    @media ( max-width: 767px ) {
      padding: 0 10px;
      display: none;
    }

    .item {
      margin: 0 5px;

      &.slick-current {
        @include opacity(.5);
      }
    }

    &:not(.slick-slider) {
      &:after {
        display: block;
        content: "";
        visibility: hidden;
        clear: both;
      }

      .item {
        {% if settings.use_color_swatch and settings.color_swatch_style == 'variant_grouped' %}
        @include opacity(0);
        {% endif %}
        display: none;
        float: left;
      }
    }
  }

  .slick-arrow {
    border: none !important;
    padding-top: 5px;
    background-color: rgba(255, 255, 255, 0) !important;
    color: $page_title_color !important;

    svg {
      width: 12px;
      height: 22px;
      stroke: rgba(255, 255, 255, 0);
    }

    &.slick-next {
      right: 0;
      text-align: right;
    }

    &.slick-prev {
      left: 0;
      text-align: left;
    }
  }

  &.vertical-moreview {
    .slider-nav {
      margin-bottom: 15px;

      .item {
        &.slick-current {
          @include opacity(.7);
        }
      }
    }
  }

  .slick-dots {
    li {
      margin: 0px 5px 7px;
      width: 9px;
      height: 9px;

      button {
        width: 9px;
        height: 9px;
        background: #B0C9D3;
        border-color: #B0C9D3;
      }

      &.slick-active {
        button {
          border-color: #005272;
          background: #005272;
        }
      }
    }
  }
}
