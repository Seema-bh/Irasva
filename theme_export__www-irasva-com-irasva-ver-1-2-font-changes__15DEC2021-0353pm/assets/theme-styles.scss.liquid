      color: $breadcrumb_color;
      font-weight: normal;
      letter-spacing: normal;
      text-transform: capitalize;

      &:before {
        background-color: $breadcrumb_color;
        bottom: 1px;
      }
    }
  }

  .widget-title {
    &.open {
      .sidebar-title:before {
        @include rotate(180);
        top: .5px;
        left: -1px;
      }
    }
  }

  .filter-color {
    .widget-content {
      margin-bottom: 35px;
