  }
}

label {
  @include label();

  em {
    color: #f1152f;
    font-weight: 500;
    font-style: normal;
    margin-left: 3px;
  }
}

ul,
ol {
  padding: 0;
  list-style: none;
  margin-bottom: 20px;

  li {
    margin-bottom: 0.25em;
  }
}

a {
  @include transition(all .3s ease);
  color: $link_color;
  font-size: 16px;
  &:hover {
    color: $link_color_hover;
    outline: none;
    text-decoration: none;
  }
}

.errors ul {
  padding-left: 0;
  text-align: center;
}

.form-success {
  border: 1px solid $border_color_1;
  margin-bottom: 20px;
  padding: 5px 20px 5px 30px;
}

/* ----- Typography -------  */
h1, h2, h3, h4, h5, h6 {
  font-family: $header_font_regular;
  line-height: 1.25em;
  margin: 0;
  padding: 0;
}
p {
  letter-spacing:0;
}

h1 {
  font-size: 72px;
  margin-bottom: 1em;
}

h2 {
  font-size: 42px;
  margin-bottom: 1em;
}

h3 {
  font-size: 36px;
  margin-bottom: 1.25em;
}

h4 {
  font-size: 30px;
  margin-bottom: 1.5em;
}

h5 {
  font-size:25px;
  margin-bottom: 1.5em;
}

h6 {
  font-size: 14px;
  margin-bottom: 1.5em;
}

p:last-child,
p:last-of-type {
