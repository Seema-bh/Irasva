    @include flexbox();
    @include justify-content(space-between);

    &:before {
      font-family: FontAwesome;
      content: "\f106";
      text-rendering: auto;
      -webkit-font-smoothing: antialiased;
      -moz-osx-font-smoothing: grayscale;
      display: inline-block;
      position: absolute;
      left: 0;
      top: -.5px;
      font-weight: 600;
      font-size: 17px;
      color: $body_color;
      @include transition(transform .3s linear);
    }

    .clear-all,
