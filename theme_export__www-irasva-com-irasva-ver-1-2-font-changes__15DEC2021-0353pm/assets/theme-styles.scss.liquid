      }
      .price-box {
        width: 17%;
        flex: 0 0 17%;
        text-align: center;
      }
      .quantity {
        width: 13%;
        flex: 0 0 13%;
      }
      .qty-group {
        border: 1px solid #e3e3e3;
        .custom_qty {
          background: transparent;
          border: none;
          width: 40%;
        }
        .button {
          width: 30%;
          height: 100%;
          background: transparent;
        }
      }
      .group-action {
        width: 10%;
        flex: 0 0 10%;
        justify-content: center;
