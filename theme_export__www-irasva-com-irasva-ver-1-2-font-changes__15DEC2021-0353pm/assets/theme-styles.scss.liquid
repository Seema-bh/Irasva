        padding: 3px;
        position: relative;

        img {
          @include border-radius(50%);
          width: 26px;
          height: 26px;
          object-fit: cover;
        }

        &:after {
          position: absolute;
          top: 0;
          left: 0;
          content: "";
          width: 100%;
          height: 100%;
          border: 4px solid $color_slick_arrow;
          background-color: rgba(255, 255, 255, 0);
          z-index: 2;
          @include opacity(0);
          visibility: hidden;
          @include transition(opacity .2s linear);
          @include border-radius(50%);
        }

        &:hover,
        &.active {
          &:after {
            visibility: visible;
            @include opacity(1);
          }
        }
      }
    }
  }

  .filter-size {
    .list-tags {
      @include flexbox();
      @include flex-wrap(wrap);

      li {
        -webkit-box-flex: 0;
        -ms-flex: 0 0 50%;
        flex: 0 0 50%;
