    .panel-title {
      font-size: $font_size;
      line-height: $font_size + 12;
      font-weight: 500;
      margin-bottom: 0;

      a {
        @include border-radius(1px);
        padding-right: 65px;
        position: relative;

        &:before {
          position: relative;
          top: 3px;
        }

        &:after {
          display: inline-block;
          font-size: $font_size;
          font-weight: normal;
          line-height: $font_size + 12;
          text-align: right;
          content: "show";
          color: $breadcrumb_color;
          position:absolute;
          right: 25px;
          top: 0;
          padding: 9px 0 7px;
          text-transform: capitalize;
        }

        &:not(.collapsed) {
          &:after {
            content: "hide";
            color: #fff;
          }
        }
      }
    }

    .panel-body {
      padding-top: 26px;
      padding-bottom: 10px;
      line-height: $font_size + 10;

      &:before {
        position: relative;
        top: 3.5px;
      }

