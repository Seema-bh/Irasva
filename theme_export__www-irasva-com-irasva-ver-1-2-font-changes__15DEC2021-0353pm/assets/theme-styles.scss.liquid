.main-content {
  display: block;
  min-height: 500px;
  padding-bottom: 105px;

  input[type="text"],
  input[type="password"],
  input[type="email"],
  input[type="tel"],
  textarea {
    height: initial;
    padding: 7px 12px 5px;
    border: 1px solid #ffb2af;
    color: $color_ipt;
    @include border-radius(5px);
    width: 100%;
    max-width: 350px;
    display: block;
    outline: none;
    margin: auto;
    font-weight: normal;
    font-family: $base_font_regular;
    letter-spacing: 1.8px;
    font-size: 13px;
