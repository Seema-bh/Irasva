/* ----- Home Slideshow ------- */
.slick-slider {
  img {
    margin-left: auto;
    margin-right: auto;
  }

  .slick-track {
    min-width: 100%;
  }
}

.slick-arrow {
  width: 36px;
  height: 36px;
  padding: 0;
  background-color: #fff;
  border: transparent;
  color: $color_slick_arrow;
  position: absolute;
  cursor: pointer;
  top: 50%;
  @include transform(translateY(-50%));
  z-index: 1;
  outline: none;
  @include transition(all .5s ease);
  @include border-radius(50%);

  .fa {
    font-size: 20px;
    font-weight: 600;
  }

  &.slick-prev {
    left: -2px;
    padding-top: 3px;
    padding-right: 1px;
  }

  &.slick-next {
    right: 7px;
    padding-left: 2px;
    padding-top: 3px;
  }

  &:not(.slick-disabled) {
    &:hover {
      background: #fff;
      color: #000;
      border-color: transparent;
    }
  }

  &.slick-disabled {
    @include opacity(0);
    cursor: default;
    pointer-events: none;
  }
}

.slick-dots {
  display: block;
  padding: 0;
  margin: 0;
  text-align: center;
  letter-spacing: -.33em;
  line-height: 0;

  li {
    display: inline-block;
    font-size: 0px;
    margin: 0px 7px;
    width: 12px;
    height: 12px;
    letter-spacing: normal;

    button {
      width: 12px;
      height: 12px;
      @include border-radius(50%);
      border: 1px solid #D3E0E7;
      background: #D3E0E7;
      padding: 0;
      outline: none;
      cursor: pointer;
    }

    &.slick-active button {
      background: #6A99AD;
      border-color: #6A99AD;
    }

    &:first-child {
      margin-left: 0;
    }

    &:last-child {
      margin-right: 0;
    }
  }
}

