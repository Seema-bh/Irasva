/* Style Common */
*:focus {
  outline: none;
}

button:focus {
  outline: none;
}

.wrapper-overlay {
  position: fixed;
  background-color: rgba(35,35,35,0.8);
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  cursor: pointer;
  display: none;
  z-index: 4;
}

html {
  padding-bottom: 0 !important;
  position: relative;
  -webkit-font-smoothing: antialiased;

  &.halo-modal-open,
  &.translate-overlay,
  &.cart-show,
  &.customer-show,
  &.options-show,
  &.sidebar-open {
    overflow-y: scroll;
    height:100%;
    body{
      overflow-y: hidden;
      height:100%;
    }
    .wrapper-overlay {
      display: block;
    }

  }
}

html,
body {
  font-size: $font_size;
  color: $body_color;
}

body {
  {% if settings.body_bg != '' %}
  background-color: $body_bg;
  {% endif %}
  margin: 0 auto;
  font-family: $base_font_regular;
  font-weight:400;
  line-height: $font_size + 8;
  letter-spacing: normal;
}

img {
  max-width: 100%;
  height: auto;
  border: none;
  border-radius: 12px;
}
.lazyload,
.lazyloading {
  opacity: 0;
}
.lazyloaded {
  opacity: 1;
  transition: opacity 300ms;
}
.placeholder-svg {
  display: block;
  fill: rgba(120, 129, 136, 0.15);
  stroke: rgba(120, 129, 136, 0.2);
  background-color: rgba(120, 129, 136, 0.1);
  width: 100%;
  height: 100%;
  max-width: 100%;
  max-height: 100%;
  border: 1px solid rgba(120, 129, 136, 0.2);
  stroke-width: .1px;
}

svg {
  fill: currentColor;
  stroke: currentColor;
  width: 18px;
  height: 38px;
}

fieldset,
legend {
  border: none;
}

input:-webkit-autofill {
  -webkit-box-shadow: 0 0 0 40px white inset;
  -webkit-text-fill-color: $body_color;
}

input::-ms-clear,
select::-ms-expand {
  display: none;
}

blockquote {
  border: none;
  background-color: #fafafa;
  color: $color_ipt;
  font-size: $font_size;
  line-height: $font_size + 12;
  font-style: normal;
  margin: 0;
  padding: 20px 35px;
}

input[type="image"] {
  width: auto;
}

textarea,
input[type="text"],
input[type="password"],
input[type="datetime"],
input[type="datetime-local"],
input[type="date"],
input[type="month"],
input[type="time"],
input[type="week"],
input[type="number"],
input[type="email"],
input[type="url"],
input[type="search"],
input[type="tel"],
input[type="color"] {
  margin: 0;
  @include border-radius(0px);
  -webkit-appearance: none;
  -moz-appearance: none;
  -ms-appearance: none;
  appearance: none;
}

input[type="text"],
input[type="password"],
input[type="datetime"],
input[type="datetime-local"],
input[type="date"],
input[type="month"],
input[type="time"],
input[type="week"],
input[type="number"],
input[type="email"],
input[type="url"],
input[type="search"],
input[type="tel"],
input[type="color"] {
  padding: 7px 12px 5px;
  line-height: $font_size + 10;
}

input[type="checkbox"],
.custom-radio input[type="radio"] {
  vertical-align: top;
  margin-right: 10px;
  display: none;
}

input[type="checkbox"] + label {
  padding-left: 22px;
  cursor: pointer;
  margin-bottom: 9px;
  position: relative;
  padding-right: 9px;
  vertical-align: text-top;

  &:before {
    position: absolute;
    content: "";
    top: 2.5px;
    left: 0;
    border: 1px solid $border_checkbox;
    width: 14px;
    height: 14px;
    background-color: #fff;
  }

  &:after {
    content: '';
    width: 9px;
    height: 5px;
    position: absolute;
    top: 6px;
    left: 3px;
    border: 2px solid #232323;
    border-top: none;
    border-right: none;
    background: rgba(255, 255, 255, 0);
    @include rotate(-45);
    @include transition(all .1s);
  }
}

input[type="checkbox"]:not(:checked) + label:after {
  @include opacity(0);
  @include transform(scale(0) rotate(-45deg));
}

input[type="checkbox"]:checked + label:after,
input[type="checkbox"] + label:hover:after {
  @include opacity(1);
  @include transform(scale(1) rotate(-45deg));
}

.custom-radio input[type="radio"] + label {
  cursor: pointer;
  margin-bottom: 9px;
  position: relative;
  padding-left: 24px;

  &:before {
    position: absolute;
    content: "";
    top: 2px;
    left: 0;
    width: 14px;
    height: 14px;
    border: 1px solid #cecece;
    @include border-radius(50%);
  }

  &:after {
    position: absolute;
    content: "";
    border: 4px solid $page_title_color;
    @include border-radius(50%);
    top: 5px;
    left: 3px;
    @include transition(all .1s);
  }
}

.custom-radio input[type="radio"]:not(:checked) + label:after {
  @include opacity(0);
  @include scale(0);
}

.custom-radio input[type="radio"]:checked + label:after,
.custom-radio input[type="radio"] + label:hover:after {
  @include opacity(1);
  @include scale(1);
}

select {
  margin-bottom: 20px;
  @include border-radius(5px);
  border:1px solid #ffb2af;
  display: block;
  width: 350px;
  padding: 5px 32px 5px 12px;
  line-height: $font_size + 10;
  outline: none;
  position: relative;
  background-color: rgba(255, 255, 255, 0);
  z-index: 2;
  -webkit-appearance: none;
  -moz-appearance: none;
  -ms-appearance: none;
  appearance: none;
  min-height: 36px;

  * {
    outline: none;
    background: #fff;
    color: $color_ipt;
    border: none;
  }

  & + .icon-dropdown {
    width: 30px;
    position: absolute;
    bottom: 0;
    right: 2px;
    background-color: rgba(255, 255, 255, 0);
    height: 36px;
    top: inherit;
