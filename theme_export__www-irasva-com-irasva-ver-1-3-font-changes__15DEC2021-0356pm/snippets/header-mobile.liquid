<div class="header-mb">          
  <div class="header-mb-left header-mb-items">
    <div class="hamburger-icon svg-mb">
      <a href="#" class="icon-nav" title="Menu Mobile Icon" data-menu-mb-toogle>
        <span class="icon-line"></span>
      </a>
    </div>

  </div>

  <div class="header-mb-middle header-mb-items">
    <h1 class="header-logo">
      {% if section.settings.brand_type == 'image' and section.settings.logo != bank %}
      <a class="logo-img" href="/" title="Logo">
       <img itemprop="logo" data-src="//cdn.shopify.com/s/files/1/0312/7689/2298/files/Irasva-Logo_color.png?v=1586435828" src="//cdn.shopify.com/s/files/1/0312/7689/2298/files/Irasva-Logo_color.png?v=1586435828" alt="Irasva" class="lazyautosizes lazyloaded" data-sizes="auto" sizes="130px">
      </a>
      {% elsif section.settings.brand_text != '' %}
      <a class="logo-title" href="/">
        {{ section.settings.brand_text }}
      </a>
      {% else %}
      <a class="logo-title" href="/">
        {{ shop.name }}
      </a>
      {% endif %}
    </h1>
  </div>

  <div class="header-mb-right header-mb-items">
    <div class="video-consult-mobile tah-link svg-mb">
      <a href="/pages/try-at-home" class="video-icon-mobile" title="Video consultation">
        <img class="tah-white" src="https://cdn.shopify.com/s/files/1/0312/7689/2298/files/rsz_tah_logo-1-03.png?v=1593097703">
        <img class="tah-blue" src="https://cdn.shopify.com/s/files/1/0312/7689/2298/files/rsz_tah_logo-02.png?v=1593099044">	
      </a>
    </div>
    <div class="video-consult-mobile video-icon tah-link svg-mb">
      <a href="/pages/video-consultation" class="video-icon-mobile" title="Video consultation">
        <img class="tah-white" src="https://cdn.shopify.com/s/files/1/0312/7689/2298/files/rsz_1video_consult_logo_finall-05.png?v=1593149894">
        <img class="tah-blue" src="https://cdn.shopify.com/s/files/1/0312/7689/2298/files/rsz_icon--1.png?v=1590828936">
      </a>
    </div>
    <div class="search-mb svg-mb">
      <a href="#" title="Search Icon" class="icon-search" data-search-mobile-toggle>
        {% include 'icon-search' %}
      </a>

      <a href="javascript:void(0)" title="close" class="close close-search">
        {% include 'icon-close' %}
      </a>

      {% include 'search-bar' %}
    </div>     
  {% comment %}
  {% if shop.customer_accounts_enabled %}
  <div class="acc-mb svg-mb mobile">
    <a href="/account" title="User Icon" class="icon-user">
      <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" id="Layer_1" x="0px" y="0px" width="20px" height="20px" viewBox="0 0 20 20" enable-background="new 0 0 20 20" xml:space="preserve">
