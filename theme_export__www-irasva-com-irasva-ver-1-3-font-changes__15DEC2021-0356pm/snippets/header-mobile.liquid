              <path fill="none" stroke="#FFFFFF" stroke-miterlimit="10" d="M3.867,5.945h9.675"/>
              <path fill="none" stroke="#FFFFFF" stroke-miterlimit="10" d="M12.927,7.927H4.3L2.578,0H0"/>
            </g>
            <rect x="1225" y="34.5" display="none" fill="none" stroke="#FFFFFF" stroke-miterlimit="10" width="18" height="18"/>
          </g>
        </svg>
        {% assign count = 0 %}
        {% for item in cart.items %}
          {% capture count %}{{ count | plus:1 }}{% endcapture %}
        {% endfor %}  
        <span class="cartCount" data-cart-count>
          {{ count }}
        </span>
      </a>
    </div>
  </div>          
</div>
